{
	"manifest_version": 2,
	"name": "网易云音乐Pro",
	"version": "0.0.1",
	"description": "网易云音乐Pro",
	"content_scripts": [
   {
     "matches": ["https://music.163.com/*"],
     "css": ["content/index.css"],
     "js": ["content/index.js"],
		 "all_frames": true
   }
 ],
	"browser_action": {
		"default_popup": "popup.html"
	},
	"icons": {
		 "16": "img/logo16.png",
		 "32": "img/logo32.png",
		 "48": "img/logo48.png",
		 "128": "img/logo128.png"
	 }
}
